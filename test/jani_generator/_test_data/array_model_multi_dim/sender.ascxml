<?xml version="1.0" encoding="UTF-8"?>
<ascxml
    initial="send"
    version="1.0"
    name="sender"
    model_src=""
    xmlns="http://www.w3.org/2005/07/scxml">

    <datamodel>
        <data id="msg_array" expr="[[], [[],[[1,2,3], [4,5,6]]]]" type="int32[][][][]" />
    </datamodel>

    <state id="send">
        <transition target="end">
            <send event="send_event">
                <param name="msg" expr="msg_array" />
            </send>
        </transition>
    </state>

    <state id="end" />

</ascxml>
